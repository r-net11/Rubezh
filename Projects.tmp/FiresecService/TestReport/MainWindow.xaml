								<dxb:BarButtonItemLink BarItemName="biExportGraphic" />
								<dxb:BarButtonItemLink BarItemName="biExportXps" />
							</dxb:PopupMenu.ItemLinks>
						</dxb:PopupMenu>
					</dxb:BarSplitButtonItem.PopupControl>
				</dxb:BarSplitButtonItem>
