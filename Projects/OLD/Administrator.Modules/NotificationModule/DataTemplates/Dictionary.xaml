<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:NotificationModule.Views"
					xmlns:ViewModels="clr-namespace:NotificationModule.ViewModels">

	<DataTemplate DataType="{x:Type ViewModels:NotificationViewModel}">
		<Views:NotificationView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:NotificationMenuViewModel}">
		<Views:NotificationMenuView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:EmailDetailsViewModel}">
		<Views:EmailDetailsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:EmailConfigViewModel}">
		<Views:EmailConfigView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ZoneSelectionViewModel}">
		<Views:ZoneSelectionView />
	</DataTemplate>

</ResourceDictionary>