<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:DevicesModule.Views"
					xmlns:ViewModels="clr-namespace:DevicesModule.ViewModels">

	<DataTemplate DataType="{x:Type ViewModels:DirectionsViewModel}">
		<Views:DirectionsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:DirectionDetailsViewModel}">
		<Views:DirectionDetailsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:DirectionsMenuViewModel}">
		<Views:DirectionsMenuView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:DirectionDeviceSelectorViewModel}">
		<Views:DirectionDeviceSelectorView />
	</DataTemplate>

</ResourceDictionary>