<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:DevicesModule.Views"
					xmlns:ViewModels="clr-namespace:DevicesModule.ViewModels">

	<DataTemplate DataType="{x:Type ViewModels:DeviceParametersViewModel}">
		<Views:DeviceParametersView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:DeviceParametersMenuViewModel}">
		<Views:DeviceParametersMenuView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ParameterTemplatesViewModel}">
		<Views:ParameterTemplatesView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ParameterTemplatesMenuViewModel}">
		<Views:ParameterTemplatesMenuView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ParameterTemplateSelectationViewModel}">
		<Views:ParameterTemplateSelectationView />
	</DataTemplate> 

</ResourceDictionary>