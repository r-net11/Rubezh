<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:DevicesModule.Views"
					xmlns:ViewModels="clr-namespace:DevicesModule.ViewModels">
	
	<DataTemplate DataType="{x:Type ViewModels:GuardViewModel}">
		<Views:GuardView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:GuardMenuViewModel}">
		<Views:GuardMenuView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:GuardConfigurationViewModel}">
		<Views:GuardConfigurationView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:UserDetailsViewModel}">
		<Views:UserDetailsView />
	</DataTemplate>
	
	<DataTemplate DataType="{x:Type ViewModels:GuardSynchronizationViewModel}">
		<Views:GuardSynchronizationView />
	</DataTemplate>

</ResourceDictionary>