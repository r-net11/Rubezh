<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:Infrastructure.Plans.Views"
					xmlns:ViewModels="clr-namespace:Infrastructure.Plans.ViewModels"
					xmlns:Common="clr-namespace:Infrastructure.Plans.Designer"
					xmlns:ElementPropertiesViewModels="clr-namespace:Infrastructure.Plans.ElementProperties.ViewModels"
					xmlns:ElementPropertiesViews="clr-namespace:Infrastructure.Plans.ElementProperties.Views">

	<DataTemplate DataType="{x:Type ViewModels:ImageTextTooltipViewModel}">
		<Views:ImageTextTooltipView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ImageTextStateTooltipViewModel}">
		<Views:ImageTextStateTooltipView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:InstrumentGroupViewModel}">
