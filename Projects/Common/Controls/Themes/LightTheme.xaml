<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<LinearGradientBrush x:Key="WindowHeaderBackground" Opacity="0" StartPoint="0, 0" EndPoint="0, 1">
		<GradientStop Color="#E3EDF5" Offset="0" />
		<GradientStop Color="#C4DAF2" Offset="1" />
	</LinearGradientBrush>

	<!-- Основной фон -->
	<LinearGradientBrush x:Key="BaseWindowBackgroundBrush" StartPoint="0, 0" EndPoint="0,1">
		<GradientStop Color="#E3EDF5" Offset="0" />
		<GradientStop Color="#C4DAF2" Offset="1" />
	</LinearGradientBrush>

	<!-- Верхняя плашка входного окна -->
	<LinearGradientBrush x:Key="DialogHeaderBrush" StartPoint="0,0" EndPoint="0,1">
		<GradientStop Color="#E3EDF5" Offset="0" />
		<GradientStop Color="#C4DAF2" Offset="1" />
	</LinearGradientBrush>

	<!-- Цвета неактивных кнопок, верхней main плашки и фона лого -->
	<LinearGradientBrush x:Key="BackgroundBrush" StartPoint="0, 0" EndPoint="0,1">
		<GradientStop Color="#E3EDF5" Offset="0" />
		<GradientStop Color="#C4DAF2" Offset="1" />
	</LinearGradientBrush>

	<!-- Цвет элементов при наведении -->
	<LinearGradientBrush x:Key="HighlightedBackgoundBrush" StartPoint="0, 0" EndPoint="0,1">
		<GradientStop Color="#029CDA" Offset="0" />
		<GradientStop Color="#0386D6" Offset="1" />
	</LinearGradientBrush>

	<!-- Неизвестность -->
	<LinearGradientBrush x:Key="HighlightedBackgoundBrush2" StartPoint="0, 0" EndPoint="0,1">
		<GradientStop Color="#029CDA" Offset="0" />
		<GradientStop Color="#0386D6" Offset="1" />
	</LinearGradientBrush>

	<!-- Цвет элементов при нажатии и верхней main плашки -->
	<LinearGradientBrush x:Key="PressedBackgroundBrush" StartPoint="0, 0" EndPoint="0,1">
		<GradientStop Color="#029CDA" Offset="0" />
		<GradientStop Color="#0386D6" Offset="1" />
	</LinearGradientBrush>
	<SolidColorBrush Color="#FFFFFF" x:Key="PressedForegroungBrush" />

	<!-- Неизвестность -->
	<LinearGradientBrush x:Key="AlternativeBackgroundBrush" StartPoint="0, 0" EndPoint="0,1">
		<GradientStop Color="#E3EDF5" Offset="0" />
		<GradientStop Color="#C4DAF2" Offset="1" />
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="WindowBorderBrush" StartPoint="0, 0" EndPoint="0,1">
		<GradientStop Color="#E3EDF5" Offset="0" />
		<GradientStop Color="#C4DAF2" Offset="1" />
	</LinearGradientBrush>

	<SolidColorBrush Color="#39495C" x:Key="NavigationForegroundColor" />
	<SolidColorBrush Color="#FFFFFF" x:Key="DisabledBorderBrush" />
	<SolidColorBrush Color="#E3EDF5" x:Key="DisabledBackgroundBrush" />
	<SolidColorBrush Color="#C4DAF2" x:Key="DisabledForegroundBrush" />
	<SolidColorBrush Color="#FFFFFF" x:Key="WindowBackground_0" />
	<SolidColorBrush Color="#FFFFFF" x:Key="WindowBackground_1" />
	<SolidColorBrush Color="#39495C" x:Key="ButtonBackground_2" />
	<SolidColorBrush Color="#39495C" x:Key="ButtonBackground_1" />
	<SolidColorBrush Color="#39495C" x:Key="ButtonBackground_0" />
	<SolidColorBrush Color="#FF0000" x:Key="ButtonBackgroundColorRed" />
	<SolidColorBrush Color="#39495C" x:Key="ForegroundColor_0" />
	<SolidColorBrush Color="#39495C" x:Key="ForegroundColor_1" />
	<SolidColorBrush Color="#39495C" x:Key="ForegroundColor_2" />
</ResourceDictionary>