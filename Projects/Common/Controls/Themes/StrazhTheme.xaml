<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!-- Фон границы для кнопок -->
	<SolidColorBrush x:Key="BorderButtonBrush" Color="#4C264459" />

	<LinearGradientBrush x:Key="WindowBlacHeaderkBackgroundBrush" Opacity="0" StartPoint="0, 0" EndPoint="0, 1">
		<GradientStop Offset="0" Color="Black" />
		<GradientStop Offset=".2" Color="Black" />
		<GradientStop Offset=".4" Color="#4172A3" />
	</LinearGradientBrush>

	<!--  Основной фон  -->
	<LinearGradientBrush x:Key="BaseWindowBackgroundBrush" StartPoint="0, 0" EndPoint="0,1">
		<GradientStop Offset="0" Color="#589ccb" />
		<GradientStop Offset=".08" Color="#589ccb" />
		<GradientStop Offset="1" Color="#589ccb" />
	</LinearGradientBrush>

	<!--  Верхняя плашка входного окна  -->
	<LinearGradientBrush x:Key="DialogHeaderBrush" StartPoint="0,0" EndPoint="0,1">
		<GradientStop Offset="0" Color="LightBlue" />
		<GradientStop Offset=".02" Color="#a2cae5" />
		<GradientStop Offset=".4" Color="#a2cae5" />
	</LinearGradientBrush>

	<!--  Цвета неактивных кнопок, верхней main плашки и фона лого  -->
	<LinearGradientBrush x:Key="BackgroundBrush" StartPoint="0, 0" EndPoint="0,1">
		<GradientStop Offset="0.2" Color="#78b8e4" />
		<GradientStop Offset="0.4" Color="#78b8e4" />
		<GradientStop Offset="0.6" Color="#78b8e4" />
		<GradientStop Offset="0.85" Color="#78b8e4" />
		<GradientStop Offset="0.95" Color="#78b8e4" />
		<GradientStop Offset="1" Color="#78b8e4" />
	</LinearGradientBrush>

	<!--  Цвет элементов при наведении  -->
	<LinearGradientBrush x:Key="HighlightedBackgoundBrush" StartPoint="0, 0" EndPoint="0,1">
		<GradientStop Offset="0" Color="#fbd029" />
		<GradientStop Offset="1" Color="#fbd029" />
	</LinearGradientBrush>

	<!--  Неизвестность  -->
	<LinearGradientBrush x:Key="HighlightedBackgoundBrush2" StartPoint="0, 0" EndPoint="0,1">
		<GradientStop Offset="0" Color="#bc13c2" />
		<GradientStop Offset="1" Color="#bc13c2" />
	</LinearGradientBrush>

	<!--  Цвет элементов при нажатии и верхней main плашки  -->
	<LinearGradientBrush x:Key="PressedBackgroundBrush" StartPoint="0, 0" EndPoint="0,1">
		<GradientStop Offset="0" Color="#fbd029" />
		<GradientStop Offset="1" Color="#fbd029" />
	</LinearGradientBrush>

	<!--  Неизвестность  -->
	<LinearGradientBrush x:Key="AlternativeBackgroundBrush" StartPoint="0, 0" EndPoint="0,1">
		<GradientStop Offset="0" Color="#589ccb" />
		<GradientStop Offset="1" Color="#589ccb" />
	</LinearGradientBrush>

	<SolidColorBrush x:Key="WindowBorderBrush" Color="#286c9c" />
	<SolidColorBrush x:Key="DisabledBorderBrush" Color="#ffffff" />
	<SolidColorBrush x:Key="DisabledBackgroundBrush" Color="LightGray" />
	<SolidColorBrush x:Key="DisabledForegroundBrush" Color="Gray" />
	<SolidColorBrush x:Key="WindowBackgroundBrush" Color="White" />
	<SolidColorBrush x:Key="ButtonBackgroundColor" Color="#ffffff" />
	<SolidColorBrush x:Key="ButtonBackgroundColorDark" Color="#286c9c" />
	<SolidColorBrush x:Key="ButtonBackgroundColorBlue" Color="LightBlue" />
	<SolidColorBrush x:Key="ForegroundColorLight" Color="White" />
	<SolidColorBrush x:Key="ForegroundColorDark" Color="Black" />
</ResourceDictionary>