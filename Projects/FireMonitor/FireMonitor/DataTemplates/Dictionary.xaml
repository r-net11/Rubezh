<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:FireMonitor.Views"
					xmlns:ViewModels="clr-namespace:FireMonitor.ViewModels">

	<DataTemplate DataType="{x:Type ViewModels:UserFotterViewModel}">
		<Views:UserFotterView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ToolbarViewModel}">
		<Views:ToolbarView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:SoundViewModel}">
		<Views:SoundView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:RestartApplicationViewModel}">
		<Views:RestartApplicationView />
	</DataTemplate>

</ResourceDictionary>