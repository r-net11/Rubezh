<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Views="clr-namespace:AlarmModule.Views"
                    xmlns:ViewModels="clr-namespace:AlarmModule.ViewModels">
    
    <DataTemplate DataType="{x:Type ViewModels:AlarmGroupsViewModel}">
        <Views:AlarmGroupsView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type ViewModels:AlarmsViewModel}">
        <Views:AlarmsView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type ViewModels:InstructionViewModel}">
        <Views:InstructionView />
    </DataTemplate>
    
</ResourceDictionary>