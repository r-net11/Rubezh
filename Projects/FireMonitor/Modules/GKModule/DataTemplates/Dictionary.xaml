<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:ViewModels="clr-namespace:GKModule.ViewModels"
	xmlns:Views="clr-namespace:GKModule.Views">

	<DataTemplate DataType="{x:Type ViewModels:DevicesViewModel}">
		<Views:DevicesView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:DeviceDetailsViewModel}">
		<Views:DeviceDetailsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:GKConnectionIndicatorViewModel}">
		<Views:GKConnectionIndicatorView />
	</DataTemplate>


	<DataTemplate DataType="{x:Type ViewModels:DaySchedulesViewModel}">
		<Views:DaySchedulesView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:DaySchedulesMenuViewModel}">
		<Views:DaySchedulesMenuView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:DayScheduleDetailsViewModel}">
		<Views:DayScheduleDetailsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:DaySchedulePartDetailsViewModel}">
		<Views:DaySchedulePartDetailsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:SchedulesViewModel}">
		<Views:SchedulesView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ScheduleDetailsViewModel}">
		<Views:ScheduleDetailsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:SchedulesMenuViewModel}">
		<Views:SchedulesMenuView />
	</DataTemplate>

</ResourceDictionary>