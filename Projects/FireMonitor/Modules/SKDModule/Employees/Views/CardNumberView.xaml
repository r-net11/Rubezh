<UserControl x:Class="SKDModule.Views.CardNumberView"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:lang="clr-namespace:Localization.SKD.Views;assembly=Localization"
			 MinWidth="300" MinHeight="100">
	<StackPanel>
        <TextBlock Text="{x:Static lang:CommonViews.PutCardUSB}" TextWrapping="Wrap" Foreground="White" Margin="2" />
		<TextBox Text="{Binding Number}" Margin="2" />
	</StackPanel>
</UserControl>