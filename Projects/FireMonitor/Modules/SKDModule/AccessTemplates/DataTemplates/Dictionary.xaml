<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:SKDModule.Views"
					xmlns:ViewModels="clr-namespace:SKDModule.ViewModels">

	<DataTemplate DataType="{x:Type ViewModels:AccessTemplateDetailsViewModel}">
		<Views:AccessTemplateDetailsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:AccessTemplateFilterViewModel}">
		<Views:AccessTemplateFilterView />
	</DataTemplate>
	
	<DataTemplate DataType="{x:Type ViewModels:ApplyAccessTemplateToUserGroupViewModel}">
		<Views:ApplyAccessTemplateToUserGroupView/>
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ApplyAccessTemplateToUserGroupFilterViewModel}">
		<Views:ApplyAccessTemplateToUserGroupFilterView/>
	</DataTemplate>

</ResourceDictionary>