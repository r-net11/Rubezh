<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:AutomationModule.Views"
					xmlns:ViewModels="clr-namespace:AutomationModule.ViewModels">

	<DataTemplate DataType="{x:Type ViewModels:GlobalVariablesViewModel}">
		<Views:GlobalVariablesView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:GlobalVariablesMenuViewModel}">
		<Views:GlobalVariablesMenuView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:AddGlobalVariableDialogViewModel}">
		<Views:AddGlobalVariableDialogView />
	</DataTemplate>

</ResourceDictionary>