<UserControl x:Class="AutomationModule.Views.StringDetailsView"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 MinWidth="400" MinHeight="400">
	<TabControl>
		<TabItem Header="Основные">
			<TextBox Text="{Binding StringValue}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" TextWrapping="Wrap" AcceptsReturn="True" />
		</TabItem>
	</TabControl>
</UserControl>
