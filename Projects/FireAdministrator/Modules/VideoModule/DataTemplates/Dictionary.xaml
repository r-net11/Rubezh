<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
					xmlns:ViewModels="clr-namespace:VideoModule.ViewModels" 
					xmlns:Views="clr-namespace:VideoModule.Views">

	<DataTemplate DataType="{x:Type ViewModels:CamerasViewModel}">
		<Views:CamerasView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:CameraDetailsViewModel}">
		<Views:CameraDetailsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:CamerasMenuViewModel}">
		<Views:CamerasMenuView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:LayoutPartCameraViewModel}">
		<Views:LayoutPartCameraView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:LayoutPartPropertyCameraPageViewModel}">
		<Views:LayoutPartPropertyCameraPageView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:PreviewViewModel}">
		<Views:PreviewView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:RviSettingsViewModel}">
		<Views:RviSettingsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:RviDeviceSelectionViewModel}">
		<Views:RviDeviceSelectionView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:CameraSelectionViewModel}">
		<Views:CameraSelectionView />
	</DataTemplate>
	
	<DataTemplate DataType="{x:Type ViewModels:CamerasSelectionViewModel}">
		<Views:CamerasSelectionView />
	</DataTemplate>

</ResourceDictionary>