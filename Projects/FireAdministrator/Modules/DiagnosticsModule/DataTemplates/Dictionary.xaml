<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:DiagnosticsModule.Views"
					xmlns:ViewModels="clr-namespace:DiagnosticsModule.ViewModels">

	<DataTemplate DataType="{x:Type ViewModels:DiagnosticsViewModel}">
		<Views:DiagnosticsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ZonesViewModel}">
		<Views:ZonesView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ZoneDetailsViewModel}">
		<Views:ZoneDetailsView />
	</DataTemplate>

</ResourceDictionary>