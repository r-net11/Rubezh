<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Views="clr-namespace:DevicesModule.Views"
                    xmlns:ViewModels="clr-namespace:DevicesModule.ViewModels">

    <DataTemplate DataType="{x:Type ViewModels:ZonesViewModel}">
        <Views:ZonesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:ZoneDetailsViewModel}">
        <Views:ZoneDetailsView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type ViewModels:ZoneLogicViewModel}">
        <Views:ZoneLogicView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:ZonesSelectionViewModel}">
        <Views:ZonesSelectionView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:ZoneLogicDevicesSelectionViewModel}">
        <Views:ZoneLogicDevicesSelectionView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:ZonesMenuViewModel}">
        <Views:ZonesMenuView />
    </DataTemplate>
    
</ResourceDictionary>