<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:SettingsModule.Views"
					xmlns:ViewModels="clr-namespace:SettingsModule.ViewModels">
	
	<DataTemplate DataType="{x:Type ViewModels:GlobalSettingsViewModel}">
		<Views:GlobalSettingsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ErrorsFilterViewModel}">
		<Views:ErrorsFilterView/>
	</DataTemplate>

</ResourceDictionary>