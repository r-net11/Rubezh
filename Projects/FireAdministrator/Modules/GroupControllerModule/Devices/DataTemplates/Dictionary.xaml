<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:ViewModels="clr-namespace:GKModule.ViewModels"
	xmlns:Views="clr-namespace:GKModule.Views">
	<DataTemplate DataType="{x:Type ViewModels:LogicViewModel}">
		<Views:LogicView />
	</DataTemplate>
	<DataTemplate DataType="{x:Type ViewModels:PropertiesViewModel}">
		<Views:PropertiesView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:FirmWareUpdateViewModel}">
		<Views:FirmWareUpdateView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ConfigurationCompareViewModel}">
		<Views:ConfigurationCompareView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:AutoSearchDevicesViewModel}">
		<Views:AutoSearchDevicesView />
	</DataTemplate>

</ResourceDictionary>