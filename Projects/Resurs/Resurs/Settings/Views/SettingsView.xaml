<UserControl x:Class="Resurs.Views.SettingsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:views="clr-namespace:Resurs.Views">
	<TabControl>
		<TabItem Header="Генерация">
			<StackPanel Background="White">
				<Button Command="{Binding GenerateCommand}" Content="Сгенерировать тестовые данные" Width="300" Margin="2" />
			</StackPanel>
		</TabItem>
		<TabItem Header="Соединение с БД">
			
		</TabItem>
		<TabItem Header="Лицензирование">
			<views:LicenseView DataContext="{Binding LicenseViewModel}" />
		</TabItem>
	</TabControl>
</UserControl>