<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:ViewModels="clr-namespace:Resurs.ViewModels"
					xmlns:Views="clr-namespace:Resurs.Views">

	<DataTemplate DataType="{x:Type ViewModels:ConsumerDetailsViewModel}">
		<Views:ConsumerDetailsView />
	</DataTemplate>
	<DataTemplate DataType="{x:Type ViewModels:ConsumersFolderDetailsViewModel}">
		<Views:ConsumersFolderDetailsView />
	</DataTemplate>
	<DataTemplate DataType="{x:Type ViewModels:ConsumerChangeParentViewModel}">
		<Views:ConsumerChangeParentView />
	</DataTemplate>
	<DataTemplate DataType="{x:Type ViewModels:ReceiptViewModel}">
		<Views:ReceiptView />
	</DataTemplate>

</ResourceDictionary>