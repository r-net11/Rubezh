<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:ViewModels="clr-namespace:Resurs.ViewModels"
					xmlns:Views="clr-namespace:Resurs.Views">

	<DataTemplate DataType="{x:Type ViewModels:UsersViewModel}">
		<Views:UsersView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:UserDetailsViewModel}">
		<Views:UserDetailsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:PermissionsViewModel}">
		<Views:PermissionsView />
	</DataTemplate>

</ResourceDictionary>