<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:RubezhMonitor.Views"
					xmlns:ViewModels="clr-namespace:RubezhMonitor.ViewModels">

	<DataTemplate DataType="{x:Type ViewModels:ToolbarViewModel}">
		<Views:ToolbarView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:SoundViewModel}">
		<Views:SoundView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:UserViewModel}">
		<Views:UserView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:AutoActivationViewModel}">
		<Views:AutoActivationView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:RestartApplicationViewModel}">
		<Views:RestartApplicationView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ChangeUserViewModel}">
		<Views:ChangeUserView />
	</DataTemplate>

	<!--<DataTemplate DataType="{x:Type ViewModels:GlobalPimActivationViewModel}">
		<Views:GlobalPimActivationView />
	</DataTemplate>-->


</ResourceDictionary>