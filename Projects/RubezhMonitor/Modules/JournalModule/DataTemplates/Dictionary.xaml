<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Views="clr-namespace:JournalModule.Views"
					xmlns:ViewModels="clr-namespace:JournalModule.ViewModels">

	<DataTemplate DataType="{x:Type ViewModels:JournalViewModel}">
		<Views:JournalView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ArchiveViewModel}">
		<Views:ArchiveView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ArchiveFilterViewModel}">
		<Views:ArchiveFilterView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ArchiveSettingsViewModel}">
		<Views:ArchiveSettingsView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:ConfirmationViewModel}">
		<Views:ConfirmationView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type ViewModels:VideoViewModel}">
		<Views:VideoView />
	</DataTemplate>

</ResourceDictionary>