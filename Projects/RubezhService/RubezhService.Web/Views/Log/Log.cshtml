@model IEnumerable<RubezhService.Models.LogItem>

@{
    ViewBag.CurrentTab = "Log";
}

<table class="table">
    <thead>
        <tr>
            <th>Название</th>
            <th>Дата</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model)
        {
            <tr>
                <td><font color="@(log.IsError ? "FF0000" : "000000")">@log.Message</font></td>
                <td>@log.DateTime</td>
            </tr>
        }
    </tbody>
</table>

