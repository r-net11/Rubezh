<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Views="clr-namespace:PowerCalculator.Views"
                    xmlns:ViewModels="clr-namespace:PowerCalculator.ViewModels">

    <DataTemplate DataType="{x:Type ViewModels:MainViewModel}">
        <Views:MainView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:NewDeviceViewModel}">
        <Views:NewDeviceView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:SpecificationViewModel}">
        <Views:SpecificationView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModels:CableTypesRepositoryViewModel}">
        <Views:CableTypesRepositoryView />
    </DataTemplate>

</ResourceDictionary>